{
  "mcpServers": {
    "apk_signer": {
      "type": "local",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/github/workspace:/workspace",
        "-w",
        "/workspace",
        "ghcr.io/millionthodin16/lanbu:latest",
        "uber-apk-signer-mcp-server"
      ],
      "tools": [
        "*"
      ]
    },
    "keytool_generator": {
      "type": "local",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/github/workspace:/workspace",
        "-w",
        "/workspace",
        "ghcr.io/millionthodin16/lanbu:latest",
        "keytool-mcp-server"
      ],
      "tools": [
        "*"
      ]
    },
    "downloader": {
      "type": "local",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-v",
        "/github/workspace:/workspace",
        "-w",
        "/workspace",
        "ghcr.io/millionthodin16/lanbu:latest",
        "downloader-mcp-server"
      ],
      "tools": [
        "*"
      ]
    }
  }
}